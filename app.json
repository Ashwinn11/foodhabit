{
  "expo": {
    "name": "Gut Buddy",
    "slug": "foodhabit",
    "owner": "foodhabit",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "foodhabit",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E2345"
    },
    "ios": {
      "supportsTablet": false,
      "requireFullScreen": true,
      "bundleIdentifier": "com.foodhabit.app",
      "usesAppleSignIn": true,
      "associatedDomains": [
        "applinks:foodhabit.com"
      ],
      "config": {
        "usesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.foodhabit.app"
        ]
      }
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro",
      "output": "single",
      "build": {
        "babel": {}
      }
    },
    "plugins": [
      "expo-apple-authentication",
      "react-native-purchases",
      "@bacons/apple-targets",
      "./plugins/withWidgetVersionSync"
    ],
    "extra": {
      "eas": {
        "projectId": "a729b1be-9c50-4ae8-a28a-96cbe598b18a"
      }
    },
    "android": {
      "package": "com.foodhabit.app"
    }
  }
}