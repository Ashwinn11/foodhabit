{
  "expo": {
    "name": "Food Habit",
    "slug": "foodhabit",
    "owner": "ashwiin11",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "foodhabit",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcefde"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.foodhabit.app",
      "usesAppleSignIn": true,
      "associatedDomains": [
        "applinks:foodhabit.com"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro",
      "output": "single",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      }
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-web-browser",
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow GutScan to access your camera to scan meals"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow GutScan to access your photos to select meal images"
        }
      ]
    ]
  }
}